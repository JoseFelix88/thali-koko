	<queryString>
		<![CDATA[SELECT
LPAD(pagosplansepare.idpagocliente,6,'0') AS idpagocliente,
pagosplansepare.fechahoraemision,
pagosplansepare.valorpago,
pagosplansepare.formapago,
LPAD(plansepare.idplansepare,4,'0') AS idplansepare,
CONSULTAR_EMPLEADO(pagosplansepare.empleado) AS empleado,
IFNULL(pagosplansepare.numrecibo,"") AS numrecibo,
pagosplansepare.cntrecibido,
pagosplansepare.cntdevuelta,
pagosplansepare.valorsaldo,
clientes.nombreapellidos,
clientes.telefono,
clientes.identificacion,
clientes.direccion,
clientes.email,datos_empresa() as empresa
FROM
pagosplansepare ,
plansepare ,
clientes
WHERE
pagosplansepare.plan = plansepare.idplansepare AND
plansepare.cliente = clientes.identificacion AND
pagosplansepare.idpagocliente = $P{NUMPAGO}]]>
	</queryString>
