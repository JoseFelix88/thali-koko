	<subDataset name="TB_HIST_PROVEEDOR" uuid="bb4d12c0-8537-4de5-8f1b-287a13228dd0">
		<parameter name="fp_1" class="java.util.Date">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="fp_2" class="java.util.Date">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="id_orden" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT
LPAD(pagoproveedores.idpagoproveedores,6,'0'),
pagoproveedores.fechahoraemision,
LPAD(pagoproveedores.pedido,4,'0') AS numpedido,
pagoproveedores.valorpago,
pagoproveedores.valorsaldo,
UPPER(pagoproveedores.formapago),
pagoproveedores.cntrecibida,
pagoproveedores.cntdevuelta,
IFNULL(pagoproveedores.numrecibo,'') AS NoRecibo,
proveedores.razonsocial AS proveedor,
IF( pagoproveedores.estado = 1, 'VIGENTE', 'ANULADO') AS estado
FROM
pagoproveedores ,
ordencompras,
proveedores
WHERE
proveedores.idproveedores = ordencompras.proveedor AND
pagoproveedores.pedido = ordencompras.idordencompras AND
(DATE_FORMAT(pagoproveedores.fechahoraemision,'%Y-%m-%d') BETWEEN $P{fp_1} AND $P{fp_2}
OR pagoproveedores.pedido = $P{id_orden});]]>
		</queryString>
