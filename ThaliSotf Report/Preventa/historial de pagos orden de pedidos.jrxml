	</parameter>
	<queryString>
		<![CDATA[SELECT
LPAD(IFNULL(MAX(ordencompras.idordencompras),0)+1,4,'0') as idordencompras,
ordencompras.fechaentrega,
ordencompras.fechaemision,
proveedores.razonsocial,
clientes.nombreapellidos,
clientes.telefono,
clientes.direccion,
clientes.identificacion,
clientes.email
FROM
ordencompras
INNER JOIN proveedores ON ordencompras.proveedor = proveedores.idproveedores
INNER JOIN clientes ON ordencompras.cliente = clientes.identificacion
where (ordencompras.fechaemision between $P{fecha1} and $P{fecha2} or
 ordencompras.idordencompras = $P{idorden})]]>
	</queryString>
