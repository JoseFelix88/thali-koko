	<subDataset name="TB_HIST_CLIENTES" uuid="a6c3c44f-9741-4a90-aa3e-bfe5d67e098b">
		<parameter name="f1" class="java.util.Date">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="f2" class="java.util.Date">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="idorden" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT
LPAD(pagosclientes.idpagocliente,6,'0') AS idcomprobantepago,
pagosclientes.fechahoraemision,
pagosclientes.formapago,
LPAD(pagosclientes.ordencompra,4,'0') AS ordenPedido,
pagosclientes.numrecibo,
pagosclientes.valorpago,
pagosclientes.valorsaldo,
pagosclientes.cntrecibido,
pagosclientes.cntdevuelta,
clientes.nombreapellidos,
IF( pagosclientes.estado = 1, 'VIGENTE', 'ANULADO') AS estado,
USUARIO_SISTEMA(pagosclientes.empleado) AS empleado
FROM
pagosclientes ,
ordencompras,
clientes
WHERE
clientes.identificacion = ordencompras.cliente AND
pagosclientes.ordencompra = ordencompras.idordencompras AND
(DATE_FORMAT(pagosclientes.fechahoraemision,'%Y-%m-%d')  BETWEEN $P{f1}  AND $P{f2}  OR
pagosclientes.ordencompra =  $P{idorden})
ORDER BY LPAD(pagosclientes.ordencompra,4,'0');]]>
		</queryString>
