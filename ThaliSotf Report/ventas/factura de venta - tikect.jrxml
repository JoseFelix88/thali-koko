	<subDataset name="Detalle_Factura" uuid="31c65002-785a-45e4-853b-16dc7d2d0ebb">
		<queryString>
			<![CDATA[SELECT
concat(productos.referencia,' ',productos.descripcion,' ',
detalleventas.cantidad,' ',
format(detalleventas.valorunidad,0),' ',
format(detalleventas.totalcompra,0)) as producto
FROM
detalleventas
INNER JOIN productos ON detalleventas.producto = productos.referencia
INNER JOIN facturaventas ON detalleventas.facturaventa = facturaventas.idfacturaventas
WHERE
facturaventas.idfacturaventas =  7
ORDER BY
productos.descripcion ASC;]]>
		</queryString>
		<field name="producto" class="java.lang.String">
			<fieldDescription><![CDATA[]]></fieldDescription>
		</field>
	</subDataset>
	<queryString>
		<![CDATA[SELECT
     empresas.`Nit` AS empresas_Nit,
     empresas.`razonsocial` AS empresas_razonsocial,
     empresas.`Telefono` AS empresas_Telefono,
     empresas.`Direccion` AS empresas_Direccion,
     empresas.`mail` AS empresas_mail,
     empresas.`celular` AS empresas_celular,
     empresas.`redsocial` AS empresas_redsocial
FROM
     `empresas` empresas]]>
	</queryString>
